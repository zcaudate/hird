/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=l);
    Or(a=in[2], b=in[3], out=m);
    Or(a=in[4], b=in[5], out=n);
    Or(a=in[6], b=in[7], out=o);
    Or(a=l, b=m, out=p);
    Or(a=n, b=o, out=q);
    Or(a=p, b=q, out=out);
}