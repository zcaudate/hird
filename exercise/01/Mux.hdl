/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    Nand(a=sel,  b=b, out=c1);
    Nand(a=nsel, b=a, out=c2);
    Nand(a=c1, b=c2, out=out);
}